<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Opencart App Test Regression Suite" thread-count="4"
	parallel="tests" verbose="4">
<listeners>
		<listener
			class-name="com.qa.opencart.listeners.ExtentReportListners" />
		<listener
			class-name="com.qa.opencart.listeners.TestAllureListener" />
	</listeners>

	<test name="Login Page Test">
		
		<classes>
			<class name="com.qa.opencart.test.LoginPageTest" />
		</classes>
	</test>
	<test name="Accounts Page Test">
		
		<classes>
			<class name="com.qa.opencart.test.AccountsPageTest" />
		</classes>
	</test>
		<test name="Products Info Page Test">
		
		<classes>
			<class name="com.qa.opencart.test.ProductPageInfoTest" />
		</classes>
	</test>
		<test name="Registration Page Test">
		
		<classes>
			<class name="com.qa.opencart.test.RegistrationPageTest" />
		</classes>
	</test>



</suite>